{
  "name": "unlp-accesible-pwa",
  "scripts": {
    "presetup": "echo 'about to setup env variables, follow the guide here: https://github.com/vercel/next.js/tree/canary/examples/cms-sanity#using-the-sanity-cli'",
    "setup": "sanity init --env .env.local",
    "postsetup": "echo 'create the read token by following the rest of the guide: https://github.com/vercel/next.js/tree/canary/examples/cms-sanity#creating-a-read-token'",
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "format": "prettier --ignore-path .gitignore -u --write .",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "prepare": "is-ci && echo 'CI detected, skipping husky installation' || husky install",
    "lint-staged": "lint-staged"
  },
  "dependencies": {
    "@sanity/block-content-to-react": "^3.0.0",
    "@sanity/image-url": "1",
    "@sanity/vision": "^3.35.2",
    "next": "14.1.0",
    "next-sanity": "7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sanity": "^3.35.2",
    "styled-components": "^6.1.8"
  },
  "devDependencies": {
    "@sanity/eslint-config-studio": "^3.0.1",
    "@types/node": "^20.11.1",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "autoprefixer": "^10.4.19",
    "danger": "^11.3.1",
    "eslint": "^8",
    "eslint-config-next": "14.1.4",
    "husky": "^8.0.1",
    "is-ci": "^3.0.1",
    "lint-staged": "^13.0.1",
    "postcss": "^8.4.38",
    "prettier": "^2.6.2",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.4.2"
  }
}
